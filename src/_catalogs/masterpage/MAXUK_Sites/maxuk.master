    <script type="text/javascript">
        
        var context;
        var web;
        var submitHook = function () { return false; };				
        var siteURL = _spPageContextInfo.webAbsoluteUrl;
        var domainURL = siteURL.split("/")[2];	     
        var parentSite ="https://"+domainURL+"/sites/"+siteURL.split('/')[4]+"/";
        var siteName = siteURL.split('/')[5];
        var userName = "";
        var contentCode;
        var sWidth = screen.width;
        var sHeight = screen.height;
        var dHeight = $(document).height();
        var dWidth = $(document).width();
        var sliderPos = "";

        theForm._spOldSubmit = theForm.submit; theForm.submit = function () {
            if (!submitHook()) { this._spOldSubmit(); }
        };
        
        /* App Sidebar Slider */
        $('#slider').slideReveal({
            trigger: $("#trigger"),
            push: false,
            position: "right",
            speed: 600,
            autoEscape: false
        });  

        if(dWidth >=2100 && dWidth <3400){
            $('#slider').css('right','-1170px');
        }
        if(dWidth >=1900 && dWidth <2100){
            $('#slider').css('right','-51.5vw');
        }
        if(dWidth >1700 && dWidth <1900){
            $('#slider').css('right','-52vw');
        }
        if(dWidth >1500 && dWidth <1700){
            $('#slider').css('right','-52vw');
        }
        if(dWidth >1400 && dWidth <1500){
            $('#slider').css('right','-52vw');
        }
        if(dWidth >1300 && dWidth <1400){
            $('#slider').css('right','-52.5vw');
        }

        var reAdjust = function () {
            if (($('.knowledgeTabContainer').outerWidth()) < widthOfList()) {
                $('.scroller-right').show().css('display', 'flex');
            }
            else {
                $('.scroller-right').hide();
            }

            if (getLeftPosi() < 0) {
                $('.scroller-left').show().css('display', 'flex');
            }
            else {
                $('.item').animate({ left: "-=" + getLeftPosi() + "px" }, 'slow');
                $('.scroller-left').hide();
            }

            if(dWidth >=1900 && dWidth <2600){
                $('#slider').css('right','-1170px');
            }
            if(dWidth >1700 && dWidth <1900){
                $('#slider').css('right','-53vw');
            }                       
        }
                
        // *** Document Object Model (DOM) Loaded
        $(function () {
            var isPowerUser = false;
            localStorage.clear();

            SP.SOD.executeFunc('sp.js', 'SP.ClientContext', sharePointReady);

            $('#newContent').append(contentCode);
            
            console.log('Screen Height :' + sHeight + ' Screen Width :'+ sWidth);
            console.log('Document Height :' + dHeight + ' Document Width :' + dWidth);

            /***** Check for temp user *****/
            $().SPServices({
                operation: "GetGroupCollectionFromUser",
                userLoginName: $().SPServices.SPGetCurrentUser(),
                async: false,
                completefunc: function (xData, Status) {

                    //If the current User does belong to a specific group
                    if ($(xData.responseXML).find("Group[Name='"+siteURL.split('/')[4]+" Intranet Visitors']").length === 1) {
                        var agreement = getCookie();

                        //***** remove search function *****/
                        var disclaimURL = 'https://'+ domainURL + '/SitePages/disclaimer.html';

                        //checks for cookie and displays disclaimer alert if new user
                        if (agreement === "") {
                            window.open(disclaimURL, '_self', 'toolbar=no,status=no,titlebar=no,scrollbars=no');
                        }

                        $('#restrictApp').removeClass('hidden');	
                            
                        console.log('Danger Will Robinson! temp user logged in');
                    }
                }
            }); 

            $.ajax({
                url: siteURL + "/_api/web/title",
                method: "GET",
                headers: { "Accept": "application/json; odata=verbose" },
                success: function (data) {
                    siteTitle = data.d.Title;
                    isPowerUser = checkPowerUser(siteTitle);
                    switch(siteName){
                        case undefined:
                            siteName = siteURL.split('/')[4];
                            getTiles(siteURL);
                            getLinkData(siteURL, siteTitle);
                            getTeamLinkData(siteURL,siteTitle);
                            if(isPowerUser){
                                $('#editLinks').append('<a href="'+parentSite+'/Lists/Useful%20Links/'+siteName+'.aspx" target="_blank" title="Edit Useful Links List"><i class="fas fa-edit"></i></a>');
                                $('#editTiles').append('<a href="'+siteURL+'/Lists/Metrotiles/AllItems.aspx?viewid=98f33d3e-29c0-4e23-8506-d7a3715b4634" target="_blank" title="Edit Metrotiles List"><i class="fas fa-edit"></i></a>');                                
                                $('#editSlides').append('<a href="'+siteURL+'/Lists/Slides/current.aspx" target="_blank" title="Edit Slides"><i class="fas fa-edit"></i></a>');
                            }
                            break;
                        case 'about':
                        case 'knowledge':
                            var rootURL="https://"+domainURL+"/sites/"+siteURL.split('/')[4]+"/";
                            console.log("rooturl="+rootURL);
                            getTiles(rootURL);
                            getLinkData(siteURL, siteTitle);
                        default:
                            getTeamTiles(siteURL);
                            getLinkData(siteURL, siteTitle);
                            getTeamLinkData(siteURL,siteTitle);
