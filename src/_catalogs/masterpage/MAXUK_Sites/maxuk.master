        // gets the Cookie if it exists
        function getCookie() {
            if (document.cookie.length > 0) {
                c_name = "PartnerAgreement";
                c_start = document.cookie.indexOf(c_name + "=")

                if (c_start !== -1) {
                    c_start = c_start + c_name.length + 1
                    c_end = document.cookie.indexOf(";", c_start)
                    if (c_end === -1) { c_end = document.cookie.length }
                    return agreement = unescape(document.cookie.substring(c_start, c_end))
                }
            }           
            return "";
        }

        function getCurrentUser() {
            //***** Get name of user from SharePoint user profile service and display on screen

            var tenantDomain = siteURL.split('/')[2];
            var tenant = tenantDomain.split('.')[0];            
            var userID = _spPageContextInfo.userId;
            var requestUri = _spPageContextInfo.webAbsoluteUrl + "/_api/SP.UserProfiles.PeopleManager/GetMyProperties"; //"/_api/web/getuserbyid(" + userid + ")";
            var requestHeaders = { "accept" : "application/json;odata=verbose" };

            $.ajax({
                url: requestUri,
                type: "GET",
                contentType : "application/json;odata=verbose",
                headers : requestHeaders,
                success : onSuccess,
                error : onError
            });
       
            function onSuccess(data) {

                var firstName = "";
                var lastName = "";
                var dataResults = data.d.UserProfileProperties.results;
                var displayName = data.d.DisplayName;
                for (var i = 0; i < dataResults.length; i++) {
                    var property = dataResults[i];
                    //console.log(property.Key);
                    if (property.Key === "FirstName") {
                        firstName = property.Value;
                    }
                    if (property.Key === "UserName"){
                        userName = property.Value;
                    }
                }
                var myURL = "https://"+[tenantDomain.slice(0, tenant.length), "-my.", tenantDomain.slice(tenant.length+1)].join('');
                var myApps = "https://www.office.com/?auth=2&home=1&username="+userName+"&from=ShellLogo";
                //var myOneDrive = "https://maximusukdev-my.sharepoint.com/_layouts/15/MySite.aspx?MySiteRedirect=AllDocuments";
                var myDelve="https://gbr.delve.office.com/";

                //if (displayName.indexOf('@') !== -1) { userName = displayName.split('@')[0] }
                //console.log(firstName+"."+lastName+" "+userName+" "+myApps);

                $("#welcomeMsg").append("Welcome " + firstName);
                $('#myOneDriveLink').prop('href',myURL);
                $('#myAppsLink').prop('href',myApps);
                $('#myDelveLink').prop('href',myDelve);

                //return dataResults.Title; //.split(' ')[0];
            }

            function onError(error) {
                console.log(error);
            }

            //$("#welcomeMsg").append("Welcome " + "<a href='"+myURL+"' target='_blank'>" + userName + "</a>"); 
        }

